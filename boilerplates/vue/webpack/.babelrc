{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "stage-0",
    "flow"
  ],
  "plugins": ["transform-runtime", "transform-decorators-legacy"],
  "env": {
    "test": {
      "presets": ["env", "stage-0"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
